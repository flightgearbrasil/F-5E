      <x> 350 </x>
      <y> 0 </y>
      <z> 0 </z>
    </location>
    <capacity unit="LBS"> 1459.85 </capacity>
    <contents unit="LBS"> 1450.00 </contents>
  </tank>
  <!-- 2.17384	0.880859	0 -->
  <tank type="FUEL">    <!-- Tank number 2 - Aft Cell -->
