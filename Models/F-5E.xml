<!-- A wrapper file for the F-5E 3D model -->

<PropertyList>

	<path>f5e.ac</path>

	<!-- Livery System -->
	<nasal>
		<load>
			var livery_update = aircraft.livery_update.new("Aircraft/F-5E/Models/Liveries", 10);
			setprop("/sim/rendering/refl_correction", 0);
			setprop("/sim/rendering/refl-type", 1);
			setprop("/sim/rendering/fresnel-factor", 0.6);
		</load>

		<unload>
			livery_update.stop();
		</unload>
	</nasal>

