<?xml version="1.0"?>
<PropertyList>

 <path>attitude-mod1.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <object-name>rollarrows</object-name>
  <property>/orientation/roll-deg</property>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/pitch-deg</property>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/heading-deg</property>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>turnrate</object-name>
  <property>/instrumentation/turn-indicator/indicated-turn-rate</property>
  <factor>0.005</factor>
  <min-m>-0.01</min-m>
  <max-m>0.01</max-m>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


